@page
@model ResetPasswordConfirmationModel
@{
    ViewData["Title"] = "Potwierdzenie resetowania hasła";
}

<h1>@ViewData["Title"]</h1>
<p>
@*     Your password has been reset. Please <a asp-page="./Login">click here to log in</a>. *@
    Twoje hasło zostało zresetowane. Proszę <a asp-page="./Login">kliknij tutaj, aby się zalogować</a>.
</p>
